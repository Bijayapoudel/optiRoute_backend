{
  "name": "optiroute_backend",
  "version": "1.0.0",
  "description": "OPTIROUTE",
  "homepage": ".",
  "scripts": {
    "start:dev": "nodemon --exec babel-node -- src/index.js",
    "start": "npx knex migrate:latest && npx knex seed:run && babel-node -- src/index.js",
    "clean": "rimraf node_modules && rimraf dist && npm install",
    "migrate": "knex migrate:latest",
    "migrate:make": "knex migrate:make",
    "migrate:rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "seed:make": "knex seed:make",
    "lint": "eslint client server; exit 0"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "javascript",
    "node",
    "es6",
    "express",
    "webpack"
  ],
  "author": "Bijaya Poudel<poudelbijaya25@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@hapi/boom": "^9.1.1",
    "bcrypt": "^5.0.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "bookshelf": "^1.2.0",
    "classnames": "^2.2.6",
    "cloudinary": "^2.4.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "eslint-plugin-react": "^7.22.0",
    "express": "^4.21.0",
    "express-handlebars": "^5.2.0",
    "fcm-node": "^1.3.0",
    "firebase-admin": "^12.4.0",
    "flatted": "^3.3.1",
    "handlebars": "^4.7.6",
    "helmet": "^4.4.1",
    "http-status-codes": "^2.1.4",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^3.1.2",
    "knex": "^3.1.0",
    "libphonenumber-js": "^1.10.54",
    "lodash": "^4.17.20",
    "method-override": "^3.0.0",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "morgan": "^1.9.1",
    "multer": "^1.4.5-lts.1",
